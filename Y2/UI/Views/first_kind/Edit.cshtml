@model Entity.config_file_first_kind

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <script src="~/Scripts/jquery-1.10.2.js"></script>
    <script src="~/Scripts/jquery.validate.min.js"></script>
    <script src="~/Scripts/jquery.validate.unobtrusive.min.js"></script>
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
</head>
<body>
    @using (Html.BeginForm("Edit", "first_kind", FormMethod.Post))
    {
        @Html.ValidationSummary(true)
        <table width="100%">
            <tr>
                <td>
                    <font color="black">
                        您正在做的业务是：人力资源--客户化设置--人力资源档案管理设置--I级机构设置--I级机构变更
                    </font>
                </td>
            </tr>
            <tr>
                <td align="right">
                    <input type="submit" value="提交" class="BUTTON_STYLE1" />
                    <input type="button" value="返回" class="BUTTON_STYLE1"
                           onclick="history.back();">
                </td>
            </tr>
        </table>

        <table width="100%" border="1" cellpadding=0 cellspacing=1
               bordercolorlight=#848284 bordercolordark=#eeeeee
               class="TABLE_STYLE1">
            <input type="hidden" name="cffk.ffkId" value="755" />
            <tr>
                <td width="19%" class="TD_STYLE1">
                    I级机构编号
                </td>
                <td width="81%" class="TD_STYLE2">
               
                    @Html.EditorFor(model => model.first_kind_id, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                    @Html.ValidationMessageFor(model => model.first_kind_id)
                </td>
                <td style="display:none">@Html.EditorFor(model => model.ffk_id, new { htmlAttributes = new { @class = "form-control" } })</td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    I级机构名称
                </td>
                <td class="TD_STYLE2">
                    @Html.EditorFor(model => model.first_kind_name, new { htmlAttributes = new { @class = "form-control", @readonly = "readonly" } })
                    @Html.ValidationMessageFor(model => model.first_kind_name)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    薪酬发放责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    @Html.EditorFor(model => model.first_kind_salary_id, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.first_kind_salary_id)
                </td>
            </tr>
            <tr>
                <td class="TD_STYLE1">
                    销售责任人编号（多个编号之间请用"半角逗号"加"一个空格"隔开，如", "）
                </td>
                <td class="TD_STYLE2">
                    @Html.EditorFor(model => model.first_kind_sale_id, new { htmlAttributes = new { @class = "form-control" } })
                    @Html.ValidationMessageFor(model => model.first_kind_sale_id)
                </td>
            </tr>
        </table>

    }
</body>
</html>


