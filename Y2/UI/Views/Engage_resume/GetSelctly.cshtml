    @using (Html.BeginForm("GetShenhe", "Engage_resume", FormMethod.Post, new { @name = "aa" }))
    {
        engage_interview ei = ViewBag.ei2;

        <input type="hidden" name="pass_register" value="@Model.pass_register" />
        <input type="hidden" name="pass_regist_time" id="a" value="@Model.pass_regist_time" />
        <input type="hidden" name="pass_register" value="@Model.checker" />
        <input type="hidden" name="pass_regist_time" id="a" value="@Model.check_status" />

            <input type="hidden" name="pass_regist_time" id="a" value="@Model.check_time" />

            <input type="hidden" name="pass_check_time" id="nowTimeaa" />

            <input type="hidden" name="operate" value="doAdopt">

            <input type="hidden" name="res_id" value="@Model.res_id" />

            <table width="100%">

                <tr>
                    <td>
                        <font color="black">您正在做的业务是：人力资源--招聘管理--录用管理--录用审核  </font>
                    </td>
                </tr>
                <tr>
                    <td align="right">
                        <input type="button" value="返回" class="BUTTON_STYLE1"
                               onclick="history.back();">

                    </td>

                </tr>
            </table>
            <table width="100%" border="1" cellpadding=0 cellspacing=1
                   bordercolorlight=#848284 bordercolordark=#eeeeee
                   class="TABLE_STYLE1">
                <tr>
                    <td class="TD_STYLE1" width="10%">
                        职位分类
                    </td>
                    <td width="10%" class="TD_STYLE2">
                        <input type="hidden" name="human_major_kind_id" value="@Model.human_major_kind_id" />

                        <input type="text" name="human_major_kind_name" value="@Model.human_major_kind_name" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        职位名称
                    </td>
                    <td width="15%" class="TD_STYLE2">
                        <input type="hidden" name="human_major_id" value="@Model.human_major_id" />

                        <input type="text" name="human_major_name" value="@Model.human_major_name" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td width="10%" class="TD_STYLE1">
                        招聘类型
                    </td>
                    <td width="20%" class="TD_STYLE2" colspan="2">
                        <input type="text" name="engage_type" value="@Model.engage_type" readonly="readonly" class="INPUT_STYLE2">
                    </td>

                    <td width="10%" rowspan="6" align="center"></td>

                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        姓名
                    </td>
                    <td class="TD_STYLE2">

                        <input type="text" name="human_name" value="@Model.human_name" readonly="readonly" class="INPUT_STYLE2">

                    </td>
                    <td class="TD_STYLE1">
                        性别
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_sex" value="@Model.human_sex" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        EMAIL
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" name="human_email" value="@Model.human_email" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        电话号码
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_telephone" value="@Model.human_telephone" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        家庭电话
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_homephone" value="@Model.human_homephone" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        手机
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" name="human_mobilephone" value="@Model.human_mobilephone" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        住址
                    </td>
                    <td colspan="3" class="TD_STYLE2">
                        <input type="text" name="human_address" value="@Model.human_address" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        邮编
                    </td>
                    <td colspan="2" class="TD_STYLE2">
                        <input type="text" name="human_postcode" value="@Model.human_postcode" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        国籍
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_nationality" value="@Model.human_nationality" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        出生地
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_birthplace" value="@Model.human_birthplace" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        生日
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <input type="text" name="human_birthday" value="@Model.human_birthday" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>

                <tr>
                    <td width="11%" class="TD_STYLE1">
                        民族
                    </td>
                    <td class="TD_STYLE2" width="14%">
                        <input type="text" name="human_race" value="@Model.human_race" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        宗教信仰
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_religion" value="@Model.human_religion" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        政治面貌
                    </td>
                    <td class="TD_STYLE2" colspan="2">
                        <input type="text" name="human_party" value="@Model.human_party" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>
                <tr>

                    <td class="TD_STYLE1">
                        身份证号码
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_idcard" value="@Model.human_idcard" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        年龄
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_age" value="@Model.human_age" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        毕业学校
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_college" value="@Model.human_college" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1" width="10%">
                        学历
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_educated_degree" value="@Model.human_educated_degree" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        教育年限
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_educated_years" value="@Model.human_educated_years" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        学历专业
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_educated_major" value="@Model.human_educated_major" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        薪酬要求
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="demand_salary_standard" value="@Model.demand_salary_standard" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        注册时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="regist_time" value="@Model.regist_time" readonly="readonly" class="INPUT_STYLE2">

                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        特长
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_specility" value="@Model.human_specility" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        爱好
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="human_hobby" value="@Model.human_hobby" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        <span style="font-size: 10.5pt; font-family: 宋体;"></span>
                        推荐人
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="checker" value="@Model.checker" readonly="readonly" class="INPUT_STYLE2">
                    </td>
                    <td class="TD_STYLE1">
                        <span style="font-size: 10.5pt; font-family: 宋体;"></span>
                        推荐时间
                    </td>
                    <td class="TD_STYLE2">
                        <input type="text" name="check_time" value="@Model.check_time" readonly="readonly" class="INPUT_STYLE2">

                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        个人履历
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <textarea name="human_history_records" rows="4"
                                  class="TEXTAREA_STYLE1" readonly="readonly">
                            @Model.human_history_records
                        </textarea>
                    </td>
                </tr>
                <tr>
                    <td class="TD_STYLE1">
                        备注
                    </td>
                    <td colspan="7" class="TD_STYLE2">
                        <textarea name="remark" rows="4" class="TEXTAREA_STYLE1" readonly="readonly">@Model.remark </textarea>
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        筛选推荐意见
                    </td>
                    <td class="TD_STYLE2" colspan="7">
                        <textarea name="recomandation" rows="4" class="TEXTAREA_STYLE1" readonly="readonly">@Model.recomandation</textarea>
                    </td>
                </tr>

                <tr>
                    <td class="TD_STYLE1">
                        面试次数
                    </td>
                    <td class="TD_STYLE2">
                        <input type="hidden" name="ein_id" value="@ei.ein_id" class="INPUT_STYLE2" readonly="readonly" />

                        <input type="text" name="interview_amount" value="@ei.interview_amount" class="INPUT_STYLE2" readonly="readonly" />
                    </td>
                    <td class="TD_STYLE1">
                        形象评价
                    </td>
                    <td class="TD_STYLE2">

                        <input type="text" name="image_degree" value="@ei.image_degree" class="INPUT_STYLE2" readonly="readonly" />

                    </td>
                    <td class="TD_STYLE1">
