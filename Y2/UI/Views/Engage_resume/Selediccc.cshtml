@model Entity.engage_resume

@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title>My JSP 'resume-details.jsp' starting page</title>
    <link rel="stylesheet"
          href="../../../css/table.css" type="text/css">
    <link rel="stylesheet"
          href="../../../css/cwcalendar.css" type="text/css">
    <script type="text/javascript"
            src="../../../javascript/comm/comm.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/comm/list.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/calendar-ch.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/jquery-1.7.2.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/locate.js">
    </script>
    <script type="text/javascript"
            src="../../../javascript/select.js">
    </script>
    <script type="text/javascript">

    </script>
    <script type="text/javascript">
 		function search(){
 //		document.getElementById("checkTime").value = document.getElementById("nowTime").value;
        document.humanfileForm.submit();
 		}
    </script>
</head>

<body>

    @*< form name="humanfileForm" method="post" action="/HR_Fist/recruit/recruitAction!updateResume">*@

    <table width="100%">
        <tr>
            <td>
                <font color="black">
                    您正在做的业务是：人力资源--招聘管理--有效简历查询--详细
                </font>
            </td>
        </tr>
        <tr>
            <td align="right">

                <input type="button" value="返回" class="BUTTON_STYLE1"
                       onclick=history.back();>
            </td>
        </tr>
    </table>
    <table width="100%" border="1" cellpadding=0 cellspacing=1
           bordercolorlight=#848284 bordercolordark=#eeeeee
           class="TABLE_STYLE1">
        <tr>
            <td class="TD_STYLE1">
                职位分类
            </td>
            <td class="TD_STYLE2">
                <input type="hidden" name="res_id" value="@Model.res_id" />

                <input type="hidden" name="human_major_kind_name" value="@Model.human_major_kind_name" />
                <select name="human_major_kind_id">
                    <option value="@Model.human_major_kind_id">
                        @Model.human_major_kind_name
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                职位名称
            </td>
            <td class="TD_STYLE2" width="20%">
                <input type="hidden" name="human_major_name" value="@Model.human_major_name" />

                <select name="human_major_id">
                    <option value="@Model.human_major_id">
                        @Model.human_major_name
                    </option>
                </select>
            </td>
            <td width="11%" class="TD_STYLE1">
                招聘类型
            </td>
            <td class="TD_STYLE2" colspan="2">
                <select name="engage_type" class="SELECT_STYLE1">
                    <option value="@Model.engage_type">
                        @Model.engage_type
                    </option>
                </select>
            </td>
            <td rowspan="6">
                &nbsp;
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                姓名
            </td>
            <td class="TD_STYLE2">
                <input type="text"
                       value="@Model.human_name" readonly="readonly"
                       name="human_name" class="INPUT_STYLE2" />
            </td>
            <td class="TD_STYLE1">
                性别
            </td>
            <td class="TD_STYLE2">
                <select name="human_sex" class="SELECT_STYLE1">
                    <option value="@Model.human_sex">
                        @Model.human_sex
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                EMAIL
            </td>
            <td colspan="2" class="TD_STYLE2">
                <input type="text" name="human_email"
                       value="@Model.human_email" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                电话
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="human_telephone"
                       value="@Model.human_telephone" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>
            <td class="TD_STYLE1">
                家庭电话
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="human_homephone"
                       value="@Model.human_homephone" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>
            <td class="TD_STYLE1">
                手机
            </td>
            <td colspan="2" class="TD_STYLE2">
                <input type="text" name="human_mobilephone"
                       value="@Model.human_mobilephone" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                住址
            </td>
            <td colspan="3" class="TD_STYLE2">
                <input type="text" name="human_address"
                       value="@Model.human_address" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>

            <td class="TD_STYLE1">
                邮编
            </td>
            <td colspan="2" class="TD_STYLE2">
                <input type="text" name="human_postcode"
                       value="@Model.human_postcode" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                国籍
            </td>
            <td class="TD_STYLE2">
                <select name="human_nationality" class="SELECT_STYLE1">
                    <option value="@Model.human_nationality">
                        @Model.human_nationality
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                出生地
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="human_birthplace"
                       value="@Model.human_birthplace" readonly="readonly"
                       class="INPUT_STYLE2">
            </td>
            <td class="TD_STYLE1">
                生日
            </td>
            <td width="13%" colspan="2" class="TD_STYLE2">
                <input type="text" name="human_birthday"
                       value="@Model.human_birthday" readonly="readonly"
                       class="INPUT_STYLE2" id="date_start">


            </td>

        </tr>
        <tr>
            <td width="11%" class="TD_STYLE1">
                民族
            </td>
            <td class="TD_STYLE2" width="14%">
                <select name="human_race" class="SELECT_STYLE1">
                    <option value="@Model.human_race">
                        @Model.human_race
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                宗教信仰
            </td>
            <td class="TD_STYLE2">
                <select name="human_religion" class="SELECT_STYLE1">
                    <option value="@Model.human_religion">
                        @Model.human_religion
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                政治面貌
            </td>
            <td class="TD_STYLE2" colspan="2">
                <select name="human_party" class="SELECT_STYLE1">
                    <option value="@Model.human_party">@Model.human_party</option>
                </select>
            </td>


        </tr>
        <tr>
            <td class="TD_STYLE1">
                身份证号码
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="human_idcard" readonly="readonly"
                       value="@Model.human_idcard"
                       class="INPUT_STYLE2">
            </td>
            <td class="TD_STYLE1">
                年龄
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="human_age" readonly="readonly"
                       value="@Model.human_age"
                       class="INPUT_STYLE2">
            </td>
            <td class="TD_STYLE1">
                毕业院校
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="human_college" readonly="readonly"
                       value="@Model.human_college"
                       class="INPUT_STYLE2" />
            </td>

            <td class="TD_STYLE1">
                学历
            </td>
            <td class="TD_STYLE2">
                <select name="human_educated_degree"
                        class="SELECT_STYLE1">
                    <option value="@Model.human_educated_degree">
                        @Model.human_educated_degree
                    </option>
                </select>
            </td>

        </tr>
        <tr>
            <td class="TD_STYLE1">
                教育年限
            </td>
            <td class="TD_STYLE2">
                <select name="human_educated_years"
                        class="SELECT_STYLE1">
                    <option value="@Model.human_educated_years">
                        @Model.human_educated_years
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                学历专业
            </td>
            <td class="TD_STYLE2">
                <select name="human_educated_major"
                        class="SELECT_STYLE1">
                    <option value="@Model.human_educated_major">
                        @Model.human_educated_major
                    </option>
                </select>
            </td>

            <td class="TD_STYLE1">
                薪酬要求
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="demand_salary_standard"
                       value="@Model.demand_salary_standard" readonly="readonly"
                       class="INPUT_STYLE2" />
            </td>
            <td class="TD_STYLE1">
                注册时间
            </td>
            <td class="TD_STYLE2">
                <input type="hidden" value="@Model.register" name="register">
                <input type="hidden" value="@Model.regist_time" name="regist_time">
                @Model.regist_time
            </td>

        </tr>
        <tr>

            <td class="TD_STYLE1">
                特长
            </td>
            <td class="TD_STYLE2">
                <select name="human_specility" class="SELECT_STYLE1">
                    <option value="@Model.human_specility">
                        @Model.human_specility
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                爱好
            </td>
            <td class="TD_STYLE2">
                <select name="human_hobby" class="SELECT_STYLE1">
                    <option value="@Model.human_hobby">
                        @Model.human_hobby
                    </option>
                </select>
            </td>
            <td class="TD_STYLE1">
                推荐人
            </td>
            <td class="TD_STYLE2">
                <input type="text" name="checker"
                       value="@Model.checker" readonly="readonly"
                       class="INPUT_STYLE2" />

            </td>
            <td class="TD_STYLE1">
                推荐时间
            </td>
            <td class="TD_STYLE2">
                <input type="hidden" name="check_time"
                       id="tt" readonly="readonly"
                       class="INPUT_STYLE2">
                @Model.check_time

            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                个人履历
            </td>
            <td colspan="7" class="TD_STYLE2">
                <textarea name="human_history_records" rows="4" readonly="readonly"
                          class="TEXTAREA_STYLE1">
                    @Model.human_history_records
                </textarea>
            </td>
        </tr>

        <tr>
            <td class="TD_STYLE1">
                备注
            </td>
            <td colspan="7" class="TD_STYLE2">
                <textarea name="remark" rows="4" readonly="readonly" class="TEXTAREA_STYLE1">
                    @Model.remark
                </textarea>
            </td>
        </tr>
        <tr>
            <td class="TD_STYLE1">
                推荐意见
            </td>
            <td colspan="7" class="TD_STYLE2">
                <textarea name="recomandation" rows="4" readonly="readonly" class="TEXTAREA_STYLE1">@Model.recomandation</textarea>
            </td>
        </tr>
    </table>
    @*</form>*@

</body>

</html>
