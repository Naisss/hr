                                    url: "/major_change/FindSalary_sum/null?standard_id=" + $("#newSalaryStandardId  option:selected").val(),
                                    type: "post",
                                    success: function (data) {
                                        var data = eval("(" + data + ")");
                                        xinchousum.val(data[0]["salary_sum"]);
                                    }
                                })
                                if (zhiwei.val() == "0") {
                                    xinchousum.val("");
                                }
                            });
                        });
                    </script>
