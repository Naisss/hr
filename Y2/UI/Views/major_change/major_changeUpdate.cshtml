                            });
                            $("#newSalaryStandardId").change(function () {
                                var Nsal = $("#newSalaryStandardId option:checked").text();
                                $("#newSalaryStandardName").val(Nsal);
                            });
                        });
                        $(function () {
                            var first = $("#firstKindId");
                            var second = $("#secondKindId");
                            var third = $("#thirdKindId");
                            var zhikind = $("#newMajorKindId");
                            var zhiname = $("#newMajorId");
                            var xinchou = $("#newSalaryStandardId");
                            var xinchousum = $("#newSalarySum");

                            first.change(function () {
                                $.ajax({
                                    url: "/major_change/Find2ji/null?firstid=" + $('#firstKindId option:selected').val(),
                                    type: "post",
                                    success: function (data) {
                                        var data = eval("(" + data + ")");
                                        second.empty();
                                        second.append("<option value='0' selected='selected'>-----请选择-----</option>");
                                        for (var i = 0; i < data.length; i++) {
                                            second.append("<option value='" + data[i]["second_kind_id"] + "'>" + data[i]["second_kind_name"] + "</option>");
                                        }
                                    }
                                });
                                if (first.val() == '0') {
                                    third.empty();
                                    second.empty();
                                    third.append("<option value='0'selected='selected'>-----请选择-----</option>");
                                    second.append("<option value='0'selected='selected'>-----请选择-----</option>")
                                }
                            });
                            second.change(function () {
                                $.ajax({
                                    url: "/major_change/Find3ji/null?secondid=" + $('#secondKindId option:selected').val(),
                                    type: "post",
                                    success: function (data) {
                                        var data = eval("(" + data + ")");
                                        third.empty();
                                        third.append("<option value='0' selected='selected'>-----请选择-----</option>");
                                        for (var i = 0; i < data.length; i++) {
                                            third.append("<option value='" + data[i]["third_kind_id"] + "'>" + data[i]["third_kind_name"] + "</option>");
                                        }
                                    }
                                });
                                if (second.val() == '0') {
                                    third.empty();
                                    third.append("<option value='0'selected='selected'>-----请选择-----</option>");
                                }
                            });
                            zhikind.change(function () {
                                $.ajax({
                                    url: "/major_change/FindMJ/null?mkid=" + $("#newMajorKindId  option:selected").val(),
                                    type: "post",
                                    success: function (data) {
                                        var data = eval("(" + data + ")");
                                        zhiname.empty();
                                        zhiname.append("<option value='0' selected='selected'>-----请选择-----</option>");
                                        for (var i = 0; i < data.length; i++) {
                                            zhiname.append("<option value='" + data[i]["major_id"] + "'>" + data[i]["major_name"] + "</option>");
                                        }
                                    }
                                })
                                if (zhiwei.val() == "0") {
