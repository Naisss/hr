@*@model List<config_file_third_kind>*@
@model DataTable
@{
    Layout = null;
}
<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link href="~/css/table.css" rel="stylesheet" />
    <link href="~/css/calendar-win2k-cold-1.css" rel="stylesheet" />
    <script src="~/javascript/cal.js"></script>
    <script src="~/javascript/comm.js"></script>
    <script src="~/javascript/list.js"></script>
    <script src="~/javascript/jquery-1.7.2.js"></script>
    <script src="~/javascript/calendar-ch.js"></script>
    <script src="~/javascript/time.js"></script>
    <link href="~/css/cwcalendar.css" rel="stylesheet" />
</head>
<body>
    <div> 
              
                    <table width="100%">
                        <tr>
                            <td>
                                <font color="black">您正在做的业务是：人力资源--调动管理--调动登记查询 </font>
                            </td>
                        </tr>
                        <tr>
                            <td align="right">
                                <input id="cx" type="submit" value="查询"
                                       class="BUTTON_STYLE1">
                            </td>
                        </tr>
                    </table>
                    <input type="hidden" name="page.startPage" value="0">
                    <table width="101%" border="1" cellpadding=0 cellspacing=1
                           bordercolorlight=#848284 bordercolordark=#eeeeee
                           class="TABLE_STYLE1">
                        <tr class="TR_STYLE1">
                            <td width="13%" class="TD_STYLE1">
                                请选择员工所在I级机构
                            </td>
                            <td width="29%" class="TD_STYLE2">
                                <select style="width:160px" name="first_kind_id" id="firstKindId" size="1" class="SELECT_STYLE2">
                                    <option value="0">-----请选择-----</option>
                                    @foreach (DataRow hm  in Model.Rows)
                                    {
                                        <option   value=@hm["first_kind_id"]>@hm["first_kind_name"]</option>
                                    }
                                </select>
                            </td>
                            <td width="13%" class="TD_STYLE1">
                                请选择员工所在II级机构
                            </td>
                            <td width="45%" class="TD_STYLE2">
                                <select style="width:160px" name="second_kind_id" id="secondKindId" size="1" class="SELECT_STYLE2">
                                    <option value="0">-----请选择-----</option>
                                    @*@foreach (DataRow hm in Model.Rows)
                                    {
                                        <option  value=@hm["second_kind_id"]>@hm["second_kind_name"]</option>
                                    }*@
                                </select>
                            </td>
                            <td width="0%"></td>
                        </tr>
                        <tr class="TR_STYLE1">
                            <td width="13%" class="TD_STYLE1">
                                请选择员工所在III级机构
                            </td>
                            <td width="29%" class="TD_STYLE2">
                                <select style="width:160px" name="third_kind_id" id="thirdKindId" size="1" class="SELECT_STYLE2">
                                    <option value="0">-----请选择-----</option>
                                    @*@foreach (DataRow  hm  in   Model.Rows)
                                    {
                                        <option   value="@hm["third_kind_id"]">@hm["third_kind_name"]</option>
                                    }*@
                                </select>
                            </td>
                            <td height="18" class="TD_STYLE1">
                                请输入建档时间
                            </td>
                            <td width="45%" class="TD_STYLE2">
                                <input name="utilbean.startDate" onclick="aa('utilbean.startDate')" type="text" class="INPUT_STYLE2" id="date_start" style="width:24% " size="30">
                                至<input type="text" name="utilbean.endDate" onclick="aa('utilbean.endDate')"  style="width:24% " class="INPUT_STYLE2" id="date_end">
                                （YYYY-MM-DD）
                            </td>
                        </tr>
                    </table>
              
    </div>
    <script>

        @{
            DataTable first = ViewBag.first;
            DataTable second = ViewBag.second;
            DataTable third = ViewBag.third;
        }
     	$(function() {
			var first=$("#firstKindId");
			var second = $("#secondKindId");
			var third=$("#thirdKindId");
	 		first.change(function(){
			    $.ajax({
			        url: "/major_change/Find2/null?firstid=" + $('#firstKindId option:selected').val(),
			        type: "post",
			        success: function (data) {
			        var data = eval("(" + data + ")");
			        second.empty();
			        second.append("<option value='0' selected='selected'>-----请选择-----</option>");
			        for (var i = 0;i<data.length;i++){
			            second.append("<option value='" + data[i]["second_kind_id"] + "'>" + data[i]["second_kind_name"] + "</option>");
			        }
			        }
			    });
	 		 	if (first.val() == '0') {
	 		    third.empty();
	 		    second.empty();
	 		    third.append("<option value='0'selected='selected'>-----请选择-----</option>");
	 		    second.append("<option value='0'selected='selected'>-----请选择-----</option>")
	 		 	}
	 		});
	 		second.change(function(){
	 		    $.ajax({
	 		        url: "/major_change/Find3/null?secondid=" + $('#secondKindId option:selected').val(),
	 		        type: "post",
	 		        success: function (data) {
	 		            var data = eval("(" + data + ")");
	 		            third.empty();
	 		            third.append("<option value='0' selected='selected'>-----请选择-----</option>");
	 		            for (var i = 0; i < data.length; i++) {
	 		                third.append("<option value='" + data[i]["third_kind_id"] + "'>" + data[i]["third_kind_name"] + "</option>");
	 		            }
	 		        }
	 		    });
	 		});
	 		$("#cx").click(function () {
	 		    var startDate = $("#date_start").val();
	 		    var endDate = $("#date_end").val();
	 		    alert(startDate);
	 		    $.ajax({
	 		        url: '/major_change/Show',
                    type:'post',
                    data: "",
                    success: function (msg) {
                        if(msg=="ok"){
                        window.location.href = "/major_change/CX/?startDate=" + startDate + "&endDate=" + endDate + "&fid=" + $("#firstKindId").val() + "&sid=" + $("#secondKindId").val() + "&tid=" + $("#thirdKindId").val();
                    }
                    }
	 		    })
	 		});
     	   });
   </script>
</body>
</html>
