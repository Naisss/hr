@model Entity.human_file

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <link rel="stylesheet"
          href="../../css/table.css" type="text/css">
    <script type="text/javascript"
            src="../../javascript/comm/comm.js">
    </script>
</head>
<body>
    <form method="post" action="/Human_file/TopImg/?id=@Model.huf_id" enctype="multipart/form-data">
        <table width="100%">
            <tr>
                <td colspan="2">
                    <font color="black">您正在做的业务是：人力资源--人力资源档案管理--图片上传</font>
                </td>
            </tr>
            <tr>
                <td width="59%">
                    提交成功，如有照片，请选择上传照片(.jpg、.gif文件)
                    <input type="file" class="FILE_STYLE1" name="human_picture"
                           width="100%" value="@Model.human_picture"/>
                    <input type="hidden" class="FILE_STYLE1" name="huf_id"
                           value="@Model.huf_id" />
                </td>
                <td width="41%" align="right">
                    <input type="button" value="上传附件" class="BUTTON_STYLE1" onclick="location.href = '/Human_file/Top_file_file/@Model.huf_id'" />
                    <input type="submit" value="完成" class="BUTTON_STYLE1" />
                </td>
            </tr>
        </table>
    </form>
</body>
</html>

